#include <iostream>
using namespace std;

int main(){
int b, j, i, k;
int a[10]={1,2,6,8,12,14,16};
i=0;
j=9-1;
b=8;
do
{
    k=(i+j)/2;
    if(a[k]<=b)
        i=k+1;
    if(a[k]>=b)
        j=k-1;
}
while(i<=j);
if(a[k]==b)
{
    cout << "ELEMENTO ENCONTRADO" << endl;
    cout << "Posicion: " << k+1;
}
else {
    cout << "ELEMENTO NO ENCONTRADO";
}

return 0;
}
